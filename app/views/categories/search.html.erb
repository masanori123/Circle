        <!-- インクリメンタルサーチ -->
        <div id="yjMain">
          <article class="section-a">
            <div class="container">
              <header class="header header--section">
                <h2 class="text-middle">
                  <i class="icon-movie color-gray-light"></i>ワードで検索
                </h2>
              </header>
              <%= form_tag('/categories/search', method: :get) do %>
                <input class="search__query" name="keyword" placeholder="ユーザー名で検索" type="text" value="">
                <button class="btn-search" name="検索" type="submit">検索</button>
                </input>
              <% end %>
              <form accept-charset="utf-8" action="/" class="js-search-submit" method="get">
              </form>
            </div>
          </article>
          <article class="section">
            <div class="container">
              <header class="header header--section">
                <h2 class="text-middle" id="text-result">ユーザーリスト</h2>
              </header>
              <ul class="listview js-lazy-load-images">
                <% @users.each do |user| %>
                <li class="user-li">
                  <!-- 飛ぶリンク先を個別になるようにprefixで記述する -->
                  <%= link_to  user_path(user.id), class: "listview__element--right-icon" do %>
                    <div class="profile-image">
                        <% if user.image.present? %>
                          <%= image_tag user.image, class: "image-a"  %>
                        <% else %>
                          <div class="no-profile-image">No Image</div>
                        <% end %>
                      </div>
                    <div class="col6 push6">
                      <h3 class="text-middle text-break">
                        <span class="color-sub" style="width:26em;">ユーザー名：<%= user.name %></span>
                      </h3>
                      <p class="text-xsmall" style="width:26em;">
                        チーム紹介：<%= user.body %>
                      </p>
                    </div>
                  <% end %>
                </li>
                <% end %>
              </ul>
            </div>
          </article>
